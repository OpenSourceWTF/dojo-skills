{
  "skills": {
    "mcp-neo4j-aura-api": {
      "name": "Neo4j MCP (Aura API)",
      "source": "github:neo4j-contrib/mcp-neo4j",
      "description": "Neo4j graph database server (schema + read/write-cypher) and separate graph database backed memory",
      "tags": [
        "mcp",
        "neo4j",
        "mcp",
        "knowledge graph",
        "aura",
        "Databases"
      ],
      "versions": {
        "latest": "main"
      },
      "mcp_servers": [
        {
          "name": "mcp-neo4j-aura-api-uvx",
          "command": "uvx",
          "args": [
            "mcp-neo4j-aura-manager",
            "--client-id",
            "${NEO4J_CLIENT_ID}",
            "--client-secret",
            "${NEO4J_CLIENT_SECRET}"
          ],
          "env": {}
        }
      ]
    },
    "mcp-neo4j-memory": {
      "name": "Neo4j MCP (Memory)",
      "source": "github:neo4j-contrib/mcp-neo4j",
      "description": "Neo4j graph database server (schema + read/write-cypher) and separate graph database backed memory",
      "tags": [
        "mcp",
        "neo4j",
        "mcp",
        "knowledge graph",
        "Databases"
      ],
      "versions": {
        "latest": "main"
      },
      "mcp_servers": [
        {
          "name": "mcp-neo4j-memory-uvx",
          "command": "uvx",
          "args": [
            "mcp-neo4j-memory",
            "--db-url",
            "${NEO4J_URI}",
            "--username",
            "${NEO4J_USERNAME}",
            "--password",
            "${NEO4J_PASSWORD}"
          ],
          "env": {}
        }
      ]
    },
    "mcp-neo4j": {
      "name": "Neo4j MCP",
      "source": "github:neo4j-contrib/mcp-neo4j",
      "description": "This server enables running Cypher graph queries, analyzing complex domain data, and automatically generating business insights that can be enhanced with Claude's analysis when an Anthropic API key is provided.",
      "tags": [
        "mcp",
        "neo4j",
        "mcp",
        "cypher",
        "knowledge graph",
        "Databases"
      ],
      "versions": {
        "latest": "main"
      },
      "mcp_servers": [
        {
          "name": "mcp-neo4j-uvx",
          "command": "uvx",
          "args": [
            "mcp-neo4j-cypher",
            "--db-url",
            "${NEO4J_URI}",
            "--username",
            "${NEO4J_USERNAME}",
            "--password",
            "${NEO4J_PASSWORD}"
          ],
          "env": {}
        }
      ]
    }
  }
