{
  "skills": {
    "neo4j": {
      "name": "Neo4j Server",
      "source": "github:da-okazaki/mcp-neo4j-server",
      "description": "A community built server that interacts with Neo4j Graph Database.",
      "tags": [
        "mcp",
        "neo4j",
        "database",
        "Databases"
      ],
      "versions": {
        "latest": "main"
      },
      "mcp_servers": [
        {
          "name": "neo4j-npm",
          "command": "npx",
          "args": [
            "-y",
            "@alanse/mcp-neo4j-server"
          ],
          "env": {
            "NEO4J_URI": "${NEO4J_URI}",
            "NEO4J_USERNAME": "${NEO4J_USERNAME}",
            "NEO4J_PASSWORD": "${NEO4J_PASSWORD}"
          }
        }
      ]
    }
  }
}
