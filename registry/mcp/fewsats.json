      "tags": [
        "mcp",
        "payments",
        "wallet",
        "offers",
        "Finance"
      ],
      "versions": {
        "latest": "main"
      },
      "mcp_servers": [
        {
          "name": "fewsats-mcp-uvx",
          "command": "uvx",
          "args": [
            "fewsats-mcp"
          ],
          "env": {
            "FEWSATS_API_KEY": "${FEWSATS_API_KEY}"
          }
        }
      ]
    }
  }
}
