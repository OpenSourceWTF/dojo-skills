    "ssh-exec": {
      "name": "SSH Execution",
      "source": "github:pathintegral-institute/mcp.science",
      "description": "A Model Context Protocol (MCP) server for executing command-line operations on remote servers via SSH.",
      "tags": [
        "mcp",
        "ssh",
        "command execution",
        "remote systems",
        "System Tools"
      ],
      "versions": {
        "latest": "main"
