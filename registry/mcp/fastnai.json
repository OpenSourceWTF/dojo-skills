      "tags": [
        "mcp",
        "Fastn",
        "Dynamic Tool Registration",
        "API-Driven Operations",
        "MCP Tools"
      ],
      "versions": {
        "latest": "main"
      },
      "mcp_servers": [
        {
          "name": "fastn-ai-unified-api-mcp-server-uvx",
          "command": "uvx",
          "args": [
            "--from",
            "git+https://github.com/fastnai/mcp-fastn",
            "fastn",
            "--api_key",
            "${YOUR_API_KEY}",
            "--space_id",
            "${YOUR_WORKSPACE_ID}"
          ],
          "env": {}
        }
      ]
    }
  }
}
