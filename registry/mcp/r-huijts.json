{
  "skills": {
    "ns-travel-information": {
      "name": "NS Travel Information",
      "source": "github:r-huijts/ns-mcp-server",
      "description": "Access Dutch Railways (NS) real-time train travel information and disruptions through the official NS API.",
      "tags": [
        "mcp",
        "NS",
        "Train",
        "Travel",
        "Information",
        "Professional Apps"
      ],
      "versions": {
        "latest": "main"
      },
      "mcp_servers": [
        {
          "name": "ns-travel-information-npm",
          "command": "npx",
          "args": [
            "-y",
            "ns-mcp-server"
          ],
          "env": {
            "NS_API_KEY": "${NS_API_KEY}"
          }
        }
      ]
    },
    "rijksmuseum": {
