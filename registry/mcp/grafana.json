{
  "skills": {
    "mcp-grafana": {
      "name": "Grafana MCP Server",
      "source": "github:grafana/mcp-grafana",
      "description": "A [Model Context Protocol][mcp] (MCP) server for Grafana.",
      "tags": [
        "mcp",
        "grafana",
        "mcp",
        "model context protocol",
        "Analytics"
      ],
      "versions": {
        "latest": "main"
      },
      "mcp_servers": [
        {
          "name": "mcp-grafana-cli",
          "command": "mcp-grafana",
          "args": [],
          "env": {
            "GRAFANA_URL": "${GRAFANA_URL}",
            "GRAFANA_API_KEY": "${GRAFANA_API_KEY}"
          }
        },
        {
          "name": "mcp-grafana-docker",
          "command": "docker",
          "args": [
            "run",
            "-i",
            "--rm",
            "-e",
            "GRAFANA_URL",
            "-e",
            "GRAFANA_API_KEY",
            "mcp/grafana",
            "--transport=stdio"
          ],
          "env": {
            "GRAFANA_URL": "${GRAFANA_URL}",
            "GRAFANA_API_KEY": "${GRAFANA_API_KEY}"
          }
        }
      ]
    }
  }
}
