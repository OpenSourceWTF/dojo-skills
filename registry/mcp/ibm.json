      ],
      "versions": {
        "latest": "main"
      },
      "mcp_servers": [
        {
          "name": "wxflows-custom",
          "command": "node",
          "args": [
            "build/index.js"
          ],
          "env": {
            "WXFLOWS_APIKEY": "${WXFLOWS_APIKEY}",
