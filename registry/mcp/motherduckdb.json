{
  "skills": {
    "mcp-server-motherduck": {
      "name": "MotherDuck MCP Server",
      "source": "github:motherduckdb/mcp-server-motherduck",
      "description": "Query and analyze data with MotherDuck and local DuckDB",
      "tags": [
        "mcp",
        "SQL",
        "DuckDB",
        "MotherDuck",
        "analytics",
        "database",
        "Databases"
      ],
      "versions": {
        "latest": "main"
      },
      "mcp_servers": [
        {
          "name": "mcp-server-motherduck-uvx",
          "command": "uvx",
          "args": [
            "mcp-server-motherduck",
            "--db-path",
            "${db-path}",
            "--motherduck-token",
            "${motherduck-token}"
          ],
          "env": {}
        }
      ]
    }
  }
}
